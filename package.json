{
  "name": "kontrakpro",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "build": "react-router build",
    "start": "wrangler pages dev",
    "deploy": "npm run build && wrangler pages deploy",
    "typecheck": "react-router typegen && tsc",
    "cf:dev": "wrangler pages dev ./build/client --compatibility-date=2024-01-01",
    "cf:deploy": "wrangler pages deploy ./build/client",
    "db:migrate": "wrangler d1 execute kontrakpro-db --local --file=./migrations/schema.sql",
    "db:migrate:prod": "wrangler d1 execute kontrakpro-db --file=./migrations/schema.sql",
    "db:shell": "wrangler d1 execute kontrakpro-db --local --command",
    "setup:cloudflare": "node scripts/setup-cloudflare.mjs"
  },
  "dependencies": {
    "@react-router/cloudflare": "^7.1.0",
    "@react-router/node": "^7.1.0",
    "@react-router/serve": "^7.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.0",
    "isbot": "^5.1.0",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.4",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slider": "^1.2.2",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@tanstack/react-query": "^5.59.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "date-fns": "^4.1.0",
    "hono": "^4.6.13",
    "lucide-react": "^0.454.0",
    "react-hook-form": "^7.54.1",
    "recharts": "^2.15.0",
    "sonner": "^1.7.1",
    "tailwind-merge": "^2.5.5",
    "zod": "^3.24.1",
    "@hookform/resolvers": "^3.9.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.15.2",
    "@cloudflare/workers-types": "^4.20241127.0",
    "@react-router/dev": "^7.1.0",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.2",
    "vite": "^6.0.1",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^3.94.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
